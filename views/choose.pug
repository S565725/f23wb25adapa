extends layout

block content
  h1= Choose
  p Hello choose.pug #{title}
  -const image_names=['img1.jpg','img2.png','img3.jpg','img4.jpg','img5.jpg','img6.jpg'] 
  - for (let i = 0; i < 3; i++)
